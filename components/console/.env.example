# MIDAZ CONSOLE ENVIRONMENT
# VERSION: Console UI version string.
VERSION=v3.4.1
# NODE_ENV: Node environment. Values: development|production|test.
NODE_ENV=development

# OPEN TELEMETRY
# ENABLE_TELEMETRY: Enable OpenTelemetry. Values: true|false.
ENABLE_TELEMETRY=false
# OTEL_RECEIVER_HTTP_PORT: OTLP HTTP receiver port.
OTEL_RECEIVER_HTTP_PORT=4318
# OTEL_HOST: OTLP collector host.
OTEL_HOST='localhost'
# OTEL_URL_METRICS: OTLP metrics endpoint.
OTEL_URL_METRICS='http://${OTEL_HOST}:${OTEL_RECEIVER_HTTP_PORT}/v1/metrics'
# OTEL_URL_TRACES: OTLP traces endpoint.
OTEL_URL_TRACES='http://${OTEL_HOST}:${OTEL_RECEIVER_HTTP_PORT}/v1/traces'
# OTEL_URL_LOGS: OTLP logs endpoint.
OTEL_URL_LOGS='http://${OTEL_HOST}:${OTEL_RECEIVER_HTTP_PORT}/v1/logs'

# CONSOLE SERVER
# MIDAZ_CONSOLE_ENABLE_DEBUG: Enable debug UI. Values: true|false.
MIDAZ_CONSOLE_ENABLE_DEBUG=true
# MIDAZ_CONSOLE_PORT: Console HTTP port.
MIDAZ_CONSOLE_PORT=8081
# MIDAZ_CONSOLE_HOST: Console host name.
MIDAZ_CONSOLE_HOST='localhost'
# MIDAZ_CONSOLE_BASE_PATH: Console base URL.
MIDAZ_CONSOLE_BASE_PATH='http://${MIDAZ_CONSOLE_HOST}:${MIDAZ_CONSOLE_PORT}'

# NGINX (LOCAL PROXY)
# NGINX_PORT: Nginx port.
NGINX_PORT=80
# NGINX_HOST: Nginx host.
NGINX_HOST='midaz-nginx'
# NGINX_BASE_PATH: Nginx base URL.
NGINX_BASE_PATH='http://${NGINX_HOST}:${NGINX_PORT}'

# MIDAZ API
# MIDAZ_API_HOST: Onboarding API host.
MIDAZ_API_HOST='midaz-onboarding'
# MIDAZ_API_PORT: Onboarding API port.
MIDAZ_API_PORT=3000
# MIDAZ_BASE_PATH: Onboarding API base URL.
MIDAZ_BASE_PATH='http://${MIDAZ_API_HOST}:${MIDAZ_API_PORT}/v1'
# MIDAZ_TRANSACTION_BASE_HOST: Transaction API host.
MIDAZ_TRANSACTION_BASE_HOST='midaz-transaction'
# MIDAZ_TRANSACTION_BASE_PORT: Transaction API port.
MIDAZ_TRANSACTION_BASE_PORT=3001
# MIDAZ_TRANSACTION_BASE_PATH: Transaction API base URL.
MIDAZ_TRANSACTION_BASE_PATH='http://${MIDAZ_TRANSACTION_BASE_HOST}:${MIDAZ_TRANSACTION_BASE_PORT}/v1'

# NEXTAUTH
# NEXTAUTH_URL: External URL for NextAuth callbacks.
NEXTAUTH_URL='http://localhost:8081'
# NEXTAUTH_SECRET: Secret used to sign NextAuth tokens.
NEXTAUTH_SECRET='SECRET'

# PLUGIN AUTH
# PLUGIN_AUTH_ENABLED: Enable auth plugin. Values: true|false.
PLUGIN_AUTH_ENABLED=false
# PLUGIN_AUTH_CLIENT_ID: Auth plugin client ID.
PLUGIN_AUTH_CLIENT_ID='SECRET'
# PLUGIN_AUTH_CLIENT_SECRET: Auth plugin client secret.
PLUGIN_AUTH_CLIENT_SECRET='SECRET'
# PLUGIN_AUTH_HOST: Auth plugin host.
PLUGIN_AUTH_HOST='plugin-auth'
# PLUGIN_AUTH_PORT: Auth plugin port.
PLUGIN_AUTH_PORT=4000
# PLUGIN_AUTH_BASE_PATH: Auth plugin base URL.
PLUGIN_AUTH_BASE_PATH='http://${PLUGIN_AUTH_HOST}:${PLUGIN_AUTH_PORT}/v1'

# PLUGIN IDENTITY
# PLUGIN_IDENTITY_HOST: Identity plugin host.
PLUGIN_IDENTITY_HOST='plugin-identity'
# PLUGIN_IDENTITY_PORT: Identity plugin port.
PLUGIN_IDENTITY_PORT=4001
# PLUGIN_IDENTITY_BASE_PATH: Identity plugin base URL.
PLUGIN_IDENTITY_BASE_PATH='http://${PLUGIN_IDENTITY_HOST}:${PLUGIN_IDENTITY_PORT}/v1'

# PLUGIN FEES (API)
# NEXT_PUBLIC_PLUGIN_FEES_ENABLED: Enable fees plugin UI. Values: true|false.
NEXT_PUBLIC_PLUGIN_FEES_ENABLED=false
# PLUGIN_FEES_HOST: Fees plugin host.
PLUGIN_FEES_HOST='plugin-fees'
# PLUGIN_FEES_PORT: Fees plugin port.
PLUGIN_FEES_PORT=4002
# PLUGIN_FEES_PATH: Fees plugin base URL.
PLUGIN_FEES_PATH='http://${PLUGIN_FEES_HOST}:${PLUGIN_FEES_PORT}/v1'

# PLUGIN FEES (FRONTEND)
# NEXT_PUBLIC_PLUGIN_FEES_FRONTEND_URL: Fees UI base URL.
NEXT_PUBLIC_PLUGIN_FEES_FRONTEND_URL='http://localhost:8084'
# NEXT_PUBLIC_PLUGIN_UI_BASE_PATH: Base path used to mount plugin UIs.
NEXT_PUBLIC_PLUGIN_UI_BASE_PATH='/plugin-fees-ui'

# MONGODB (CONSOLE DATA)
# MONGODB_URI: Mongo connection string.
MONGODB_URI='mongodb://midaz:lerian@midaz-mongodb:5703/?directConnection=true'
# MONGODB_DB_NAME: Mongo database name for console.
MONGODB_DB_NAME='midaz-console'
# MONGODB_USER: Mongo user.
MONGODB_USER='midaz'
# MONGODB_PASS: Mongo password.
MONGODB_PASS='lerian'

# NEXT PUBLIC CONFIG
# NEXT_PUBLIC_MIDAZ_CONSOLE_AVATAR_ALLOWED_FORMAT: Allowed avatar formats (comma-separated).
NEXT_PUBLIC_MIDAZ_CONSOLE_AVATAR_ALLOWED_FORMAT='jpeg,jpg,png,svg+xml'
# NEXT_PUBLIC_MIDAZ_APPLICATION_OPTIONS: Available apps (comma-separated).
NEXT_PUBLIC_MIDAZ_APPLICATION_OPTIONS=midaz,plugin-fees,plugin-crm,plugin-smart-templates
# NEXT_PUBLIC_MIDAZ_CONSOLE_BASE_URL: Public base URL (used by nginx microfrontends).
NEXT_PUBLIC_MIDAZ_CONSOLE_BASE_URL='http://localhost:8081'
# NEXT_PUBLIC_MIDAZ_AUTH_ENABLED: Expose auth enabled flag to frontend.
NEXT_PUBLIC_MIDAZ_AUTH_ENABLED=${PLUGIN_AUTH_ENABLED}
# NEXT_PUBLIC_MIDAZ_VERSION: Expose version to frontend.
NEXT_PUBLIC_MIDAZ_VERSION=${VERSION}

# ACCOUNT TYPES VALIDATION
# MIDAZ_ACCOUNT_TYPE_VALIDATION_ENABLED: Enable account type validation. Values: true|false.
MIDAZ_ACCOUNT_TYPE_VALIDATION_ENABLED=false
# MIDAZ_ACCOUNT_TYPE_VALIDATION: Optional list of <organization-id>:<ledger-id> pairs (comma-separated).
MIDAZ_ACCOUNT_TYPE_VALIDATION=
