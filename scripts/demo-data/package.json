{
  "name": "midaz-demo-data-generator",
  "version": "0.1.0",
  "description": "Realistic demo data generator for Midaz using Faker.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "generate": "ts-node --project tsconfig-ts-node.json src/index.ts",
    "generate:small": "ts-node --project tsconfig-ts-node.json src/index.ts --volume small",
    "generate:medium": "ts-node --project tsconfig-ts-node.json src/index.ts --volume medium",
    "generate:large": "ts-node --project tsconfig-ts-node.json src/index.ts --volume large",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "faker": "^5.5.3",
    "midaz-sdk": "file:midaz-sdk-typescript",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/node": "^18.19.100",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
